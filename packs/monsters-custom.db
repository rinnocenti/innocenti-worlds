nd":"// v1.0\n//console.log(args)\n\nif (args[0].hitTargets.length < 1) return {};\ntoken = canvas.tokens.get(args[0].tokenId);\nactor = token.actor;\nif (!actor || !token || args[0].hitTargets.length < 1) return {};\nlet martial = actor.items.getName(\"Martial Advantage\").data.data.formula;\n\nif (!martial) {\n    MidiQOL.warn(\"Martial Advantage Damage: Trying to do Martial Advantage and not have formula\");\n    return {};\n}\nlet target = canvas.tokens.get(args[0].hitTargets[0]._id);\nif (game.combat) {\n    let combatTime = game.combat.round + game.combat.turn / 100;\n    let lastTime = getProperty(token.data.flags, \"midi-qol.martialAdvantageTime\");\n    if (combatTime === lastTime) {\n        MidiQOL.warn(\"Martial Advantage Damage: Already done a Martial Advantage this turn\");\n        return {};\n    }\n}\nlet foundEnemy = false;\nlet isMartialAdv = false;\nfoundEnemy = false;\nlet nearbyEnemy = canvas.tokens.placeables.filter(t => {\n    let nearby = (t.actor &&\n        t?.id !== args[0].tokenId && // not me\n        t.id !== target.id && // not the target\n        t.actor?.data.data.attributes?.hp?.value > 0 && // not incapacitated\n        t.data.disposition !== target.data.disposition && // not an ally\n        MidiQOL.getDistance(t, target, false) <= 5 // close to the target\n    );\n    foundEnemy = foundEnemy || (nearby && t.data.disposition === -target.data.disposition)\n    return nearby;\n});\nisMartialAdv = nearbyEnemy.length > 0;\n\nif (!isMartialAdv) {\n    MidiQOL.warn(\"Sneak Attack DamageMartial Advantage Damage: No ally next to target\");\n    return {};\n}\n\nlet dialog = new Promise((resolve, reject) => {\n    new Dialog({\n        // localize this text\n        title: \"Conditional Damage\",\n        content: `<p>Use Martial Advanced?</p>` + (!foundEnemy ? \"<p>Only Nuetral creatures nearby</p>\" : \"\"),\n        buttons: {\n            one: {\n                icon: '<i class=\"fas fa-check\"></i>',\n                label: \"Confirm\",\n                callback: () => resolve(true)\n            },\n            two: {\n                icon: '<i class=\"fas fa-times\"></i>',\n                label: \"Cancel\",\n                callback: () => { resolve(false) }\n            }\n        },\n        default: \"two\"\n    }).render(true);\n});\nlet useMartial = await dialog;\n\nif (!useMartial) return {}\nconst diceMult = args[0].isCritical ? 2 : 1;\nlet formulapiece = martial.split('d');\nformulapiece[0] = diceMult * parseInt(formulapiece[0]);\nmartial = formulapiece.join('d');\n//const baseDice = Math.ceil(rogueLevels/2);\nif (game.combat) {\n    let combatTime = game.combat.round + game.combat.turn / 100;\n    let lastTime = getProperty(token.data.flags, \"midi-qol.martialAdvantageTime\");\n    if (combatTime !== lastTime) {\n        setProperty(token.data.flags, \"midi-qol.martialAdvantageTime\", combatTime)\n    }\n}\n\n// How to check that we've already done one this turn?\nreturn { damageRoll: `${martial}`, flavor: \"Martial Advanced\" };","author":"X4uo406iXTm4HkVN"},"data":{"name":"Martial Advantage","type":"script","scope":"global","command":"// v1.0\n//console.log(args)\n\nif (args[0].hitTargets.length < 1) return {};\ntoken = canvas.tokens.get(args[0].tokenId);\nactor = token.actor;\nif (!actor || !token || args[0].hitTargets.length < 1) return {};\nlet martial = actor.items.getName(\"Martial Advantage\").data.data.formula;\n\nif (!martial) {\n    MidiQOL.warn(\"Martial Advantage Damage: Trying to do Martial Advantage and not have formula\");\n    return {};\n}\nlet target = canvas.tokens.get(args[0].hitTargets[0]._id);\nif (game.combat) {\n    let combatTime = game.combat.round + game.combat.turn / 100;\n    let lastTime = getProperty(token.data.flags, \"midi-qol.martialAdvantageTime\");\n    if (combatTime === lastTime) {\n        MidiQOL.warn(\"Martial Advantage Damage: Already done a Martial Advantage this turn\");\n        return {};\n    }\n}\nlet foundEnemy = false;\nlet isMartialAdv = false;\nfoundEnemy = false;\nlet nearbyEnemy = canvas.tokens.placeables.filter(t => {\n    let nearby = (t.actor &&\n        t?.id !== args[0].tokenId && // not me\n        t.id !== target.id && // not the target\n        t.actor?.data.data.attributes?.hp?.value > 0 && // not incapacitated\n        t.data.disposition !== target.data.disposition && // not an ally\n        MidiQOL.getDistance(t, target, false) <= 5 // close to the target\n    );\n    foundEnemy = foundEnemy || (nearby && t.data.disposition === -target.data.disposition)\n    return nearby;\n});\nisMartialAdv = nearbyEnemy.length > 0;\n\nif (!isMartialAdv) {\n    MidiQOL.warn(\"Sneak Attack DamageMartial Advantage Damage: No ally next to target\");\n    return {};\n}\n\nlet dialog = new Promise((resolve, reject) => {\n    new Dialog({\n        // localize this text\n        title: \"Conditional Damage\",\n        content: `<p>Use Martial Advanced?</p>` + (!foundEnemy ? \"<p>Only Nuetral creatures nearby</p>\" : \"\"),\n        buttons: {\n            one: {\n                icon: '<i class=\"fas fa-check\"></i>',\n                label: \"Confirm\",\n                callback: () => resolve(true)\n            },\n            two: {\n                icon: '<i class=\"fas fa-times\"></i>',\n                label: \"Cancel\",\n                callback: () => { resolve(false) }\n            }\n        },\n        default: \"two\"\n    }).render(true);\n});\nlet useMartial = await dialog;\n\nif (!useMartial) return {}\nconst diceMult = args[0].isCritical ? 2 : 1;\nlet formulapiece = martial.split('d');\nformulapiece[0] = diceMult * parseInt(formulapiece[0]);\nmartial = formulapiece.join('d');\n//const baseDice = Math.ceil(rogueLevels/2);\nif (game.combat) {\n    let combatTime = game.combat.round + game.combat.turn / 100;\n    let lastTime = getProperty(token.data.flags, \"midi-qol.martialAdvantageTime\");\n    if (combatTime !== lastTime) {\n        setProperty(token.data.flags, \"midi-qol.martialAdvantageTime\", combatTime)\n    }\n}\n\n// How to check that we've already done one this turn?\nreturn { damageRoll: `${martial}`, flavor: \"Martial Advanced\" };","author":"X4uo406iXTm4HkVN"},"options":{},"apps":{},"compendium":null}},"core":{"sourceId":"Item.dpqx5UrVU7aw7pRt"}},"img":"systems/dnd5e/icons/skills/gray_08.jpg","effects":[{"_id":"7unBqPaWHDX5dpoO","flags":{"dae":{"stackable":false,"macroRepeat":"none","transfer":true},"ActiveAuras":{"isAura":false,"ignoreSelf":false,"hidden":false,"height":false,"alignment":"","type":"","aura":"None","radius":null,"save":"","savedc":null,"hostile":false,"onlyOnce":false,"time":"None"}},"changes":[{"key":"flags.dnd5e.DamageBonusMacro","value":"ItemMacro.Martial Advantage","mode":0,"priority":20}],"disabled":false,"duration":{"startTime":null,"seconds":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"icon":"systems/dnd5e/icons/skills/gray_08.jpg","label":"Martial Advantage","origin":"Actor.H0VChqSya6XDni7B.OwnedItem.dpqx5UrVU7aw7pRt","tint":"","transfer":true}]}],"effects":[{"_id":"kTNPG5crJMmcl1UL","flags":{"dae":{"transfer":true,"armorEffect":true}},"changes":[{"key":"data.attributes.ac.value","value":"16","mode":5,"priority":4}],"disabled":false,"duration":{"startTime":47120227200},"icon":"systems/dnd5e/icons/items/armor/scale.png","label":"AC=16","origin":"Actor.H0VChqSya6XDni7B.OwnedItem.1E010hrmxAxxxX7g"},{"_id":"iui6hCqDCI6EDDLC","flags":{"dae":{"transfer":true,"armorEffect":true}},"changes":[{"key":"data.attributes.ac.value","value":"2","mode":2,"priority":7}],"disabled":false,"duration":{"startTime":47120227200},"icon":"systems/dnd5e/icons/items/armor/shield.png","label":"AC+2","origin":"Actor.H0VChqSya6XDni7B.OwnedItem.M7tSkuLQ726VTbHM"},{"_id":"HJxKEk4FC4Ha9jyj","flags":{"dae":{"stackable":false,"macroRepeat":"none","transfer":true},"ActiveAuras":{"isAura":false,"ignoreSelf":false,"hidden":false,"height":false,"alignment":"","type":"","aura":"None","radius":null,"save":"","savedc":null,"hosti