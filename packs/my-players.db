ects":[]},{"_id":"cxPxhQxJLq6NYo8T","name":"Tinderbox","type":"loot","data":{"description":{"value":"<div>\n\t\t\t<div><i>Adventuring gear</i></div>\n\t\t\t<div class=\"row\">\n\t\t\t\t<div class=\"col-4\">5 sp, 1 lb.</div>\n\t\t\t\t<div class=\"col-8 text-right\">  </div>\n\t\t\t</div>\n\t\t</div>\n\t\t<hr>\n\t\t<div  class=\"rd__b  rd__b--2\"><p>This small container holds flint, fire steel, and tinder (usually dry cloth soaked in light oil) used to kindle a fire. Using it to light a torch - or anything else with abundant, exposed fuel - takes an action. Lighting any other fire takes 1 minute.</p></div>","chat":"","unidentified":""},"source":"PHB","quantity":1,"weight":1,"price":0.5,"attuned":false,"equipped":true,"rarity":"None","identified":false,"damage":{"parts":[]},"attributes":{"spelldc":10},"consume":{"type":"","target":"","amount":null}},"sort":7200000,"flags":{},"img":"systems/dnd5e/icons/items/inventory/torch.jpg","effects":[]},{"_id":"KDjEuk2EgAaVkrZf","name":"Torch","type":"consumable","data":{"description":{"value":"<div>\n\t\t\t<div><i>Adventuring gear</i></div>\n\t\t\t<div class=\"row\">\n\t\t\t\t<div class=\"col-4\">1 cp, 1 lb.</div>\n\t\t\t\t<div class=\"col-8 text-right\">  </div>\n\t\t\t</div>\n\t\t</div>\n\t\t<hr>\n\t\t<div  class=\"rd__b  rd__b--2\"><p>A <i>torch</i> burns for 1 hour, providing bright light in a 20-foot radius and dim light for an additional 20 feet. If you make a melee attack with a burning <i>torch</i> and hit, it deals 1 fire damage.</p></div>","chat":"","unidentified":""},"source":"PHB","quantity":0,"weight":1,"price":0.01,"attuned":false,"equipped":true,"rarity":"None","identified":false,"activation":{"type":"action","cost":1,"condition":""},"duration":{"value":1,"units":"hour"},"target":{"value":40,"units":"ft","type":"radius"},"range":{"value":null,"long":null,"units":""},"uses":{"value":1,"max":1,"per":"charges","autoDestroy":false,"autoUse":true},"consume":{"type":"","target":"","amount":null},"ability":"str","actionTy