e":"Backpack","type":"backpack","img":"/systems/dnd5e/icons/items/inventory/backpack.jpg","data":{"description":{"value":"<div>\n\t\t\t<div><i>Adventuring gear</i></div>\n\t\t\t<div class=\"row\">\n\t\t\t\t<div class=\"col-4\">2 gp, 5 lb.</div>\n\t\t\t\t<div class=\"col-8 text-right\">  </div>\n\t\t\t</div>\n\t\t</div>\n\t\t<hr>\n\t\t<div  class=\"rd__b  rd__b--2\"><p>A <i>backpack</i> can hold one cubic foot or 30 pounds of gear. You can also strap items, such as a bedroll or a coil of rope, to the outside of a <i>backpack</i>.</p></div>","chat":"","unidentified":""},"source":"PHB","quantity":1,"weight":5,"price":2,"attuned":false,"equipped":true,"rarity":"None","identified":false,"capacity":{"type":"weight","value":30,"weightless":false},"currency":{"cp":0,"sp":0,"ep":0,"gp":0,"pp":0},"damage":{"parts":[]},"attributes":{"spelldc":10},"consume":{"type":"","target":"","amount":null}},"sort":2400000},{"_id":"JAvNISLnzDr6HLAf","flags":{},"name":"Bedroll","type":"loot","img":"/systems/dnd5e/icons/items/inventory/cloth-blue.jpg","data":{"description":{"value":"<div>\n\t\t\t<div><i>Adventuring gear</i></div>\n\t\t\t<div class=\"row\">\n\t\t\t\t<div class=\"col-4\">1 gp, 7 lb.</div>\n\t\t\t\t<div class=\"col-8 text-right\">  </div>\n\t\t\t</div>\n\t\t</div>\n\t\t<hr>\n\t\t","chat":"","unidentified":""},"source":"PHB","quantity":1,"weight":7,"price":1,"attuned":false,"equipped":true,"rarity":"None","identified":false,"damage":{"parts":[]},"attributes":{"spelldc":10},"consume":{"type":"","target":"","amount":null}},"sort":2500000},{"_id":"w7fVN7fVr6pLwCtL","flags":{},"name":"Common Clothes","type":"equipment","img":"/systems/dnd5e/icons/items/equipment/tunic.jpg","data":{"description":{"value":"<div>\n\t\t\t<div><i>Adventuring gear</i></div>\n\t\t\t<div class=\"row\">\n\t\t\t\t<div class=\"col-4\">5 sp, 3 lb.</div>\n\t\t\t\t<div class=\"col-8 text-right\">  </div>\n\t\t\t</div>\n\t\t</div>\n\t\t<hr>\n\t\t","chat":"","unidentified":""},"source":"PHB","quantity":1,"weight":3,