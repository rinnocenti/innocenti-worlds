ergonhava e repreendia aqueles de quem não gostava ou que via como não cooperativos com sua autoridade.</p>"},"_id":"fPlRJV0tVjcIXe4D","image":{},"video":{"controls":true,"volume":0.5},"src":"assets/npcs/Grizzelda%20Copperwraught.webp","system":{},"sort":0,"ownership":{"default":-1},"flags":{"monks-enhanced-journal":{"type":"person","relationships":[{"type":"place","id":"PwdBuxXbbRmpExC5","relationship":"Lider","hidden":false},{"id":"0ttJXyIxS3XDdcUA","type":"person","hidden":false,"relationship":"Aliada"},{"id":"mcXGbLLfELld4Cnd","type":"poi","hidden":false,"relationship":"Lider"},{"id":"8UcXFRjOu0NvJzgc","type":"person","hidden":false,"relationship":"Aliada"},{"id":"sz6EMdsYgBpEi6og","type":"person","hidden":false,"relationship":"Aliada"},{"id":"z4lyrmJDY2yXiK2x","type":"quest","hidden":false,"relationship":"SLW"},{"id":"HfG7SVN2HFWOrktx","type":"quest","hidden":false,"relationship":"Contato"},{"id":"u5TBRatf2OgL8075","uuid":"JournalEntry.u5TBRatf2OgL8075","hidden":false}],"attributes":{"race":{"value":""},"gender":{"value":""},"age":{"value":""},"eyes":{"value":""},"skin":{"value":""},"hair":{"value":""},"life":{"value":""},"profession":{"value":""},"voice":{"value":""},"faction":{"value":""},"height":{"value":""},"weight":{"value":""},"traits":{"value":""},"ideals":{"value":""},"bonds":{"value":""},"flaws":{"value":""},"longterm":{"value":""},"shortterm":{"value":""},"beliefs":{"value":""},"secret":{"value":""}},"role":"Lider do Conselho","location":"Leilon","DPSwXpPVU8d93GBw":{"notes":""},"_lasttab":"description","scrollPos":"{\".tab.entry-details .tab-inner\":0,\".tab.description .tab-inner\":0,\".relationships .items-list\":0}"}}}],"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1673126475480,"modifiedTime":1682462668036,"lastModifiedBy":"dMfobz2VOtfW25fX"},"folder":null,"sort":0,"ownership":{"default":0,"dMfobz2VOtfW25fX":3},"_id":"nH0gBDkG5OWOPzop"}
