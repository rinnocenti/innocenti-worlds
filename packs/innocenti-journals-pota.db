{"name":"Missão dos Cavaleiros","flags":{"monks-enhanced-journal":{"type":"quest","scrollPos":"{\".objective-items\":0,\".reward-container\":0,\".tab.description .tab-inner\":0}","relationships":[{"id":"uPK9gw5zN7OfnZZS","type":"person","hidden":false,"relationship":"PotA"},{"id":"7y3aMssneSxjOecQ","type":"quest","hidden":false}],"actor":{"id":"65f5X5WrWedE7HFg","uuid":"Actor.65f5X5WrWedE7HFg","img":"tokenizer/npc-images/thurl_merosska.Avatar.webp?1641486374791","name":"Thurl Merosska","quantity":"1"},"source":"Thurl Merosska","status":"inactive","display":true,"_lasttab":"description"},"scene-packer":{"hash":"5b5d818d9caa946d7ea966168dfcf6f4e631419e"},"cf":{"id":"temp_d3w0bk2sdwp","path":"Locals, Quests and Persons#/CF_SEP/PotA#/CF_SEP/Quests","color":"#000000"},"core":{"sourceId":"Compendium.world.campanha-de-quarta.MX7werCscXEkmtxv"}},"pages":[{"name":"Missão dos Cavaleiros","type":"image","src":"assets/journal/PotA/MissaoCavaleiros.webp","title":{"show":false,"level":1},"_id":"FQ5ggO68mKjjhed6","image":{},"text":{"format":1},"video":{"controls":true,"volume":0.5},"system":{},"sort":0,"ownership":{"default":-1},"flags":{"monks-enhanced-journal":{"type":"quest","scrollPos":"{\".objective-items\":0,\".tab.description .tab-inner\":0}","relationships":[{"id":"uPK9gw5zN7OfnZZS","type":"person","hidden":false,"relationship":"PotA","name":"Thurl Merosska","img":"assets/npcs/Thurl%20Merosska.jpg"},{"id":"7y3aMssneSxjOecQ","type":"quest","hidden":false,"name":"O Pináculo Penaventaval","img":"assets/journal/PotA/pinaculo.webp","relationship":""}],"actor":{"id":"65f5X5WrWedE7HFg","uuid":"Actor.65f5X5WrWedE7HFg","img":"tokenizer/npc-images/thurl_merosska.Avatar.webp?1641486374791","name":"Thurl Merosska","quantity":"1"},"source":"Thurl Merosska","status":"inactive","display":true,"DPSwXpPVU8d93GBw":{"notes":""},"_lasttab":"description","appendix":false}}}],"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1673126474744,"modifiedTime":1