rride":null},"spell6":{"value":0,"max":0,"override":null},"spell7":{"value":0,"max":0,"override":null},"spell8":{"value":0,"max":0,"override":null},"spell9":{"value":0,"max":0,"override":null},"pact":{"value":0,"max":0,"override":null,"level":0},"spell0":{"value":0,"max":0}},"bonuses":{"mwak":{"attack":"","damage":""},"rwak":{"attack":"","damage":""},"msak":{"attack":"","damage":""},"rsak":{"attack":"","damage":""},"abilities":{"check":"","save":"","skill":""},"spell":{"dc":""}},"resources":{"legact":{"value":0,"max":0},"legres":{"value":0,"max":0},"lair":{"value":false,"initiative":20}}},"sort":100001,"flags":{"dnd5e":{"conditions":[]}},"img":"https://5e.tools/img/PotA/Stonemelder.png","token":{"flags":{},"name":"Stonemelder","displayName":20,"img":"https://5e.tools/img/PotA/Stonemelder.png","tint":null,"width":1,"height":1,"scale":1,"lockRotation":false,"rotation":0,"vision":true,"dimSight":0,"brightSight":30,"dimLight":0,"brightLight":0,"sightAngle":360,"lightAngle":360,"lightAlpha":1,"actorId":"NTDsomuxp1AigWK8","actorLink":false,"actorData":{},"disposition":-1,"displayBars":40,"bar1":{"attribute":"attributes.hp"},"bar2":{"attribute":null},"randomImg":false},"items":[{"_id":"ClUv2gXTZi1UE0qp","flags":{},"name":"Acid Splash","type":"spell","img":"modules/plutonium/media/icon/spell/phb-acid-splash.jpg","data":{"description":{"value":"<div  class=\"rd__b  rd__b--3\"><p>You hurl a bubble of acid. Choose one creature you can see within range, or choose two creatures you can see within range that are within 5 feet of each other. A target must succeed on a Dexterity saving throw or take [[/r 1d6]] acid damage.</p><div class=\"rd__spc-inline-post\"></div><p>This spell's damage increases by [[/r 1d6]] when you reach 5th level ([[/r 2d6]]), 11th level ([[/r 3d6]]), and 17th level ([[/r 4d6]]).</p></div>","chat":"","unidentified":""},"source":"PHB","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":null,"units":"inst"},"target":{"value":0,"units":""