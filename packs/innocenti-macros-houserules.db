{"name":"RecoveryExhaustian","type":"script","author":"7HItjeooBSTfAudP","img":"icons/magic/life/crosses-trio-red.webp","scope":"global","command":"console.log(\"RECOVERY\", args)\ntry {\n    //const lastArg = args[args.length-1];\n    let xtarget = await fromUuid(args[0].hitTargetUuids[0]);\n    let recovery = parseInt(args[0].item.system.formula);\n    let exha = xtarget.actor.system.attributes.exhaustion;\n    console.log(\"EXHAU\", xtarget, recovery,exha )\n    if (args[0].tag == \"OnUse\") {\n        //console.log(\"rest\",exha, recovery)\n        if (exha > 0) {\n            let nexha = ((exha - recovery) <= 0) ? 0 : (exha - recovery);\n            console.log(\"rest2\", exha, nexha)\n            await xtarget.actor.update({ 'system.attributes.exhaustion': nexha });\n            ChatMessage.create({ content: `${args[0].item.name} reduce exhaustion to ${nexha}` });\n        } else {\n            ChatMessage.create({ content: `there is no exhaust to reduce with the ${args[0].item.name}` });\n        }\n    }\n} catch (err) {\n    console.error(`Recovery Exhaustion`, err);\n}","flags":{"combat-utility-belt":{"macroTrigger":""},"advanced-macros":{"runAsGM":true,"runForSpecificUser":""},"scene-packer":{"sourceId":"Macro.28pNe0cyfS1k6S3m"},"cf":{"id":"temp_xnm02cinrio","path":"GM_DAE","color":"#000000"},"core":{"sourceId":"Compendium.innocenti-worlds.macros-houserules.zDnUqynFzGcx2uK2"},"exportSource":{"world":"foundry-template","system":"dnd5e","coreVersion":"9.280","systemVersion":"1.6.3"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.3.0","coreVersion":"10.303","createdTime":1682478993795,"modifiedTime":1692215359273,"lastModifiedBy":"7HItjeooBSTfAudP"},"_id":"REUfmtVR8wy1jGqW","folder":null,"sort":0,"ownership":{"default":0,"7HItjeooBSTfAudP":3}}
