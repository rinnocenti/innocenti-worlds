{"name":"Update Loot-Consumable","type":"script","author":"7HItjeooBSTfAudP","img":"icons/svg/dice-target.svg","scope":"global","command":"(async ()=>{\n    let changeFolder = 'Custom Foods';\n    let items = await game.items;\n    await items.map(x=>{\n        if(x.folder?.name == changeFolder ){\n          console.log(x.name);\n          x.update({type: 'consumable', 'data.consumableType': 'food', 'data.uses.autoDestroy': true});\n        }\n        \n    });\n    console.log(\"FIM\")\n})();\n\n\n\n\n    //await game.items.contents.map(item=>{\n        //if(item.name != null){\n        //console.log(item.folder.name)\n        //if(item.folder.name == changeFolder){\n           //console.log(\"Name\", item)\n        //item.update({type: 'consumable', 'data.consumableType': 'food', 'data.uses.autoDestroy': true});\n        //}\n        //}\n        \n    //})","flags":{"combat-utility-belt":{"macroTrigger":""},"advanced-macros":{"runAsGM":false},"cf":{"id":"temp_bxke4rz8ao9","path":"GM_Generator","color":"#000000"},"core":{"sourceId":"Compendium.innocenti-worlds.macros-houserules.cFmZEvLi38bkA8Xl"},"scene-packer":{"sourceId":"Macro.2n9IaMiIJX8laaxX","hash":"512b1f4c270a6426893e7f24089a6fe6db6f1234"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.3.0","coreVersion":"10.303","createdTime":1670367288471,"modifiedTime":1692215365753,"lastModifiedBy":"7HItjeooBSTfAudP"},"_id":"4X6qKAuxY6ZQRxjH","folder":null,"sort":0,"ownership":{"default":0,"7HItjeooBSTfAudP":3}}
{"name":"SizeWeapons","type":"script","author":"7HItjeooBSTfAudP","img":"icons/weapons/axes/axe-battle-orange.webp","scope":"global","command":"const version = \"10.0.17\";\n//SizeWeapons 3\n//console.log(\"SizeWeapons\", args);\nconst lastArg = args[args.length - 1];\nconst mTimes = args[1] ?? 3;\nconst actor = await fromUuid(lastArg.actorUuid, \"Actor\");\n\n\ntry {\n    if (args[0] === \"on\") {\n        const listWeapons = actor.items.filter(x => x.type === \"weapon\" && x.system.damage?.parts)\n            .map(y => {\n                let copyItem = duplicate(y.toObject(false));\n                return {\n                    '_id': copyItem._id, system: {\n                        damage: {\n                            parts: copyItem.system.damage.parts\n                        }\n                    }\n                }\n            });\n        console.log(\"WEAPOPNS\", listWeapons);\n        DAE.setFlag(actor, `SizeWeapons`, listWeapons)\n            .then(() => {\n                listWeapons.map(x => {\n                    return x.system.damage.parts.forEach(y => {\n                       y[0] =  y[0].replace(/([0-9]{1,2})(d[0-9]{1,3})/g, function (all, numb, dado) { return `${parseInt(numb) * mTimes}${dado}` });\n                       return y;\n                    });\n                });\n            actor.updateEmbeddedDocuments(\"Item\", listWeapons);\n            ChatMessage.create({ content: \"All weapons and armor increased in size\" });\n            console.log(\"WEAPON\", listWeapons);\n        });\n    }\n    if (args[0] === \"off\") {\n        const flag = DAE.getFlag(actor, `SizeWeapons`);\n        await actor.updateEmbeddedDocuments(\"Item\", flag);\n        DAE.unsetFlag(actor, `SizeWeapons`);\n        //console.log(\"WEAPON\", actor.items);\n    }\n} catch (err) {\n    console.error(`SizeWeapons ${version}`, err);\n}","flags":{"combat-utility-belt":{"macroTrigger":""},"advanced-macros":{"runAsGM":true},"cf":{"id":"temp_xnm02cinrio","path":"GM_DAE","color":"#000000"},"core":{"sourceId":"Compendium.innocenti-worlds.innocenti-macros-houserules.T5Lsit5Fb4RkYF5E"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.3.0","coreVersion":"10.303","createdTime":1669691792219,"modifiedTime":1692215359254,"lastModifiedBy":"7HItjeooBSTfAudP"},"_id":"5xJobc6RwuK6qonw","folder":null,"sort":0,"ownership":{"default":0,"7HItjeooBSTfAudP":3}}
{"name":"Change tables","type":"script","scope":"global","author":"7HItjeooBSTfAudP","img":"icons/svg/dice-target.svg","command":"//let folderName = \"Tabelas Completas\";\n//let folderName = \"Sub Merchants\";\n//let folderName = \"Merchants\"\nlet folderName = \"Tabelas Segmentadas\"\n//let newCompendium = \"innocenti-worlds.item-gemstones\";\n//let newCompendium = \"innocenti-worlds.innocenti-rolltable-merchants\";\nlet newCompendium = \"innocenti-worlds.innocenti-items-adventure\";\n//let newCompendium = \"innocenti-worlds.innocenti-items-recipes\";\nawait game.tables.map(table=>{\n    if(table.folder?.name && table.folder.name == folderName){\n        table.results.map(x=>{\n            //if( x.documentCollection == \"innocenti-worlds.item-gemstones\"){\n            //if( x.documentCollection == \"innocenti-worlds.item-custom\"){\n            //if( x.documentCollection == \"innocenti-worlds.item-custom\"){\n                if( x.documentCollection == \"world.ddb-test2-ddb-backgrounds\"){\n                x.documentCollection = newCompendium \n                console.log(\"Name\", table.name); \n            }\n            })\n       \n    }\n    \n});","flags":{"advanced-macros":{"runAsGM":false,"runForSpecificUser":""},"cf":{"id":"temp_bxke4rz8ao9","path":"GM_Generator","color":"#000000"},"core":{"sourceId":"Compendium.innocenti-worlds.innocenti-macros-houserules.6nAu63LKnoCJyt2N"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.3.0","coreVersion":"10.303","createdTime":1682518004714,"modifiedTime":1692215365757,"lastModifiedBy":"7HItjeooBSTfAudP"},"_id":"6nAu63LKnoCJyt2N","folder":null,"sort":0,"ownership":{"default":0,"7HItjeooBSTfAudP":3}}
{"name":"RemoveConditions","type":"script","scope":"global","author":"7HItjeooBSTfAudP","img":"icons/magic/symbols/ring-circle-smoke-blue.webp","command":"//RemoveConditions blinded\nconsole.log(\"CONDITION\", args);\nconst lastArg = args[args.length-1];\nconst effect = args[1];\n\nconst hasEffectApplied = await game.dfreds.effectInterface.hasEffectApplied(effect , lastArg.actorUuid);\n\nif (hasEffectApplied) {\n  game.dfreds.effectInterface.removeEffect({ effectName: effect , lastArg.actorUuid});\n}","flags":{"combat-utility-belt":{"macroTrigger":""},"advanced-macros":{"runAsGM":true},"cf":{"id":"temp_xnm02cinrio","path":"GM_DAE","color":"#000000"},"core":{"sourceId":"Compendium.innocenti-worlds.innocenti-macros-houserules.M0UehcYjAAPaaCAL"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.3.0","coreVersion":"10.303","createdTime":1668699913372,"modifiedTime":1692215359257,"lastModifiedBy":"7HItjeooBSTfAudP"},"_id":"6vYjRddU6UOjbS6C","folder":null,"sort":0,"ownership":{"default":0,"7HItjeooBSTfAudP":3}}
{"name":"Random Push","type":"script","author":"7HItjeooBSTfAudP","img":"icons/skills/movement/arrow-upward-yellow.webp","scope":"global","command":"const lastArg = args[args.length-1];\nlet target = MidiQOL.MQFromUuid(lastArg.tokenUuid)\nconsole.log(\"ARGS\", args)\nif(args[0] == \"on\"){\n    let content = \"\";\n    let r = new Roll(\"1d8\");\n    await r.evaluate({async: true});\n    let dir = DirectionToken(r.total);\n    let chatData = {\n        speaker: ChatMessage.getSpeaker(),\n        content: `<p>${target.name} has pushed <strong>${args[1]}ft</strong> away in <strong><img src=\"assets/icons/arrows/${dir}.png\" width=\"15\"></strong></p>`\n    };\n    console.log(\"dir\", dir)\n    await ChatMessage.create(chatData, {});\n}\nfunction DirectionToken(result){\n    switch (result) {\n        case 1:\n            return \"north\"\n            break;\n        case 2:\n            return \"northeast\"\n            break;\n        case 3:\n            return \"east\"\n            break;\n        case 4:\n            return \"southeast\"\n            break;\n        case 5:\n            return \"south\"\n            break;\n        case 6:\n            return \"southwest\"\n            break;\n        case 7:\n            return \"west\"\n            break;\n        case 8:\n            return \"northwest\"\n            break;\n        \n        default:\n            // code\n    }\n}","flags":{"combat-utility-belt":{"macroTrigger":""},"advanced-macros":{"runAsGM":false},"cf":{"id":"temp_xnm02cinrio","path":"GM_DAE","color":"#000000"},"core":{"sourceId":"Compendium.innocenti-worlds.macros-houserules.sUyvkun5YUI1P6i1"},"scene-packer":{"sourceId":"Macro.dYTypZmWa1pZx6pl"},"exportSource":{"world":"foundry-template","system":"dnd5e","coreVersion":"9.280","systemVersion":"1.6.3"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.3.0","coreVersion":"10.303","createdTime":1682478993788,"modifiedTime":1692215359259,"lastModifiedBy":"7HItjeooBSTfAudP"},"_id":"7rz031NMdRp9O20u","folder":null,"sort":0,"ownership":{"default":0,"7HItjeooBSTfAudP":3}}
{"name":"SetSenses","type":"script","scope":"global","author":"7HItjeooBSTfAudP","img":"icons/magic/perception/eye-slit-orange.webp","command":"console.log(\"Args\", args);\nconst version = \"10.0.18\";\n//SetSenses \"basicSight\" 120\nlet lastArg = args[args.length - 1];\nlet tokenDetect = token.document.detectionModes;\nlet newTokenSense =\n{\n    \"id\": args[1],\n    \"enabled\": true,\n    \"range\": args[2]\n}\n\ntry {\n    let sansekey = await Object.keys(tokenDetect).find(key => tokenDetect[key].id == newTokenSense.id);\n    if (args[0] === \"on\") {\n        if (tokenDetect[sansekey] && tokenDetect[sansekey].enabled == true && tokenDetect[sansekey].range == newTokenSense.range) return;        \n        DAE.setFlag(actor, `SetSenses${args[1]}`, tokenDetect[sansekey])\n            .then(async () => {\n                if (tokenDetect[sansekey])\n                    tokenDetect[sansekey] = newTokenSense;\n                else\n                    tokenDetect.push(newTokenSense);\n                await token.document.update({\n                    detectionModes: tokenDetect\n                });\n                ChatMessage.create({ content: token.name + \" Recive a new detect mode: \" + newTokenSense.id });\n                console.log(`SetSenses${args[1]}`, tokenDetect);\n            });\n    }\n    if (args[0] === \"off\") {\n        const flag = DAE.getFlag(token, `SetSenses${args[1]}`);\n        if (!flag) {\n            delete tokenDetect[sansekey];\n        } else {\n            tokenDetect[sansekey] = flag;\n        }\n        await token.document.update({\n            detectionModes: tokenDetect\n        });\n        DAE.unsetFlag(actor, `SetSenses${args[1]}`);\n        ChatMessage.create({ content: token.name + \" loose detect mode: \" + newTokenSense.id });\n    }\n} catch (err) {\n    console.error(`SetSenses${args[1]} ${version}`, err);\n}","flags":{"advanced-macros":{"runAsGM":false},"cf":{"id":"temp_xnm02cinrio","path":"GM_DAE","color":"#000000"},"core":{"sourceId":"Compendium.innocenti-worlds.innocenti-macros-houserules.AkjhE9HuDjg0qfA5"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.3.0","coreVersion":"10.303","createdTime":1671411045856,"modifiedTime":1692215359261,"lastModifiedBy":"7HItjeooBSTfAudP"},"_id":"9sGwYsNBcCTF1Bbo","folder":null,"sort":0,"ownership":{"default":0,"7HItjeooBSTfAudP":3}}
{"name":"UpdateTargetSpiderBite","type":"script","scope":"global","author":"7HItjeooBSTfAudP","img":"icons/creatures/abilities/fang-tooth-poison-green.webp","command":"console.log(\"SpidyBite\", args);\nlet lastArg = args[args.length - 1];\nconst POSTPOISON_NAME = \"Remaining Venom\";\nif (args[0] !== \"onUpdateActor\" && lastArg.actor.system.attributes.hp.value != 0) return;\nif (game.dfreds.effectInterface.hasEffectApplied(\"Poison Spider\", lastArg.tokenUuid)) {\n\n    const POSTPOISON_EFFECT = {\n        label: POSTPOISON_NAME,\n        icon: \"icons/skills/toxins/poison-bottle-corked-fire-green.webp\",\n        duration: {\n            \"seconds\": 3600,\n            \"type\": \"seconds\",\n            \"duration\": 3600,\n            \"remaining\": 3600,\n            \"label\": \"3600 Seconds\"\n        },\n        changes:\n            [\n                {\n                    \"key\": \"StatusEffect\",\n                    \"value\": \"Convenient Effect: Poisoned\",\n                    \"mode\": 0,\n                    \"priority\": 20\n                },\n                {\n                    \"key\": \"StatusEffect\",\n                    \"value\": \"Convenient Effect: Paralyzed\",\n                    \"mode\": 0,\n                    \"priority\": 20\n                }\n            ],\n        transfer: true,\n    };\n\n    await lastArgs.targetActor.createEmbeddedDocuments(\"ActiveEffect\", [POSTPOISON_EFFECT]);\n}","flags":{"advanced-macros":{"runAsGM":true,"runForSpecificUser":""},"cf":{"id":"temp_xnm02cinrio","path":"GM_DAE","color":"#000000"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.3.0","coreVersion":"10.303","createdTime":1684951597878,"modifiedTime":1692215359267,"lastModifiedBy":"7HItjeooBSTfAudP"},"_id":"BjuObQCMy5etLclk","folder":null,"sort":0,"ownership":{"default":0,"7HItjeooBSTfAudP":3}}
